Inicio
Escribir "Cual es tu sueldo base: ";
Leer sueldobase;
Escribir "Cuanto dinero has ganado con está primera venta: ";
Leer venta1;
Escribir "Cuanto dinero has ganado con está segunda venta: ";
Leer venta2;
Escribir "Cuanto dinero has ganado con está tercera venta: ";
Leer venta3;
comision1 = 10 * venta1 /100;
comision2 = 10 * venta2 /100;
comision3 = 10 * venta3 /100;
comisionfinal = comision1 + comision2 * comision3;
sueldofinal = sueldobase + comisionfinal;
Escribir "El sueldo final de este vendedor es " sueldofinal;
Fin

